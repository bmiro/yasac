procedure prova(a: in integer; b: in integer; c: in out integer; d: out character) is
	type valor is new integer range 0..5;		
   v: valor;
	type ar is array(character, character, valor) of integer;
	e: integer;
	type r is record
		r1:integer;
		r2:integer;
		r3:valor;
		r4:character;
	end record;
begin
	a := 1;
